{
  "name": "mtr-kit",
  "version": "1.13.5",
  "repository": "https://github.com/mtr-today/mtr-kit.git",
  "author": "marklai1998 <marklai1998@gmail.com>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16.0.0 <21.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "postinstall": "husky install",
    "prepare": "husky install",
    "pre-commit": "pnpm lint-staged",
    "pre-push": "tsc",
    "test": "tsc",
    "clean-package-json": "pnpm clear-package-json package.json -o package-cleaned.json --fields scripts && mv package-cleaned.json package.json",
    "lint": "eslint src"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "17.8.0",
    "@commitlint/config-conventional": "17.8.0",
    "@spotify/eslint-config": "15.0.0",
    "@types/papaparse": "5.3.9",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "clean-publish": "4.2.0",
    "esbuild": "0.19.5",
    "eslint": "8.51.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "8.0.3",
    "import-sort-cli": "6.0.0",
    "import-sort-parser-babylon": "6.0.0",
    "import-sort-parser-typescript": "6.0.0",
    "import-sort-style-module": "6.0.0",
    "lint-staged": "15.0.1",
    "prettier": "3.0.3",
    "rollup": "4.1.4",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-dts": "6.1.0",
    "rollup-plugin-esbuild": "6.1.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-visualizer": "5.9.2",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "papaparse": "^5.4.1",
    "wretch": "^2.6.0"
  }
}
