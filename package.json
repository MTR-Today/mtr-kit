{
  "name": "mtr-kit",
  "version": "1.13.8",
  "repository": "https://github.com/mtr-today/mtr-kit.git",
  "author": "marklai1998 <marklai1998@gmail.com>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "rollup -c",
    "prepare": "husky",
    "pre-commit": "pnpm biome check --fix --staged --no-errors-on-unmatched",
    "pre-push": "tsc"
  },
  "files": [
    "dist"
