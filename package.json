{
  "name": "mtr-kit",
  "version": "1.6.0",
  "repository": "https://github.com/mtr-today/mtr-kit.git",
  "author": "marklai1998 <marklai1998@gmail.com>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "^20"
  },
  "scripts": {
    "build": "rollup -c",
    "postinstall": "husky install",
    "prepare": "husky install",
    "pre-commit": "pnpm lint-staged",
    "pre-push": "tsc",
    "test": "tsc",
    "clean-package-json": "pnpm clear-package-json package.json -o package-cleaned.json --fields scripts && mv package-cleaned.json package.json",
    "lint:js": "eslint src"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@spotify/eslint-config": "15.0.0",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "clean-publish": "4.2.0",
    "esbuild": "0.18.4",
    "eslint": "8.43.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "import-sort-cli": "6.0.0",
    "import-sort-parser-babylon": "6.0.0",
    "import-sort-parser-typescript": "6.0.0",
    "import-sort-style-module": "6.0.0",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "rollup": "3.25.1",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-dts": "5.3.0",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-visualizer": "5.9.2",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "wretch": "^2.5.2"
  }
}
